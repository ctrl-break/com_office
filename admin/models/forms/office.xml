<?xml version="1.0" encoding="utf-8"?>
<form
			addrulepath="/administrator/components/com_office/models/rules"
>
	<fieldset>
		<field
				name="id"
				type="hidden"
				/>
		<field
				name="title"
				type="text"
				label="COM_OFFICE_OFFICE_TITLE_LABEL"
				description="COM_OFFICE_OFFICE_TITLE_DESC"
				size="40"
				class="inputbox"
				required="true"
				default=""
				/>

				<field
						name="catid"
						type="category"
						extension="com_office"
						class="inputbox"
						default=""
						label="COM_OFFICE_OFFICE_FIELD_CATID_LABEL"
						description="COM_OFFICE_OFFICE_FIELD_CATID_DESC"
						required="true"
				>
					<option value="0">JOPTION_SELECT_CATEGORY</option>
				</field>

		<field
				name="address"
				type="text"
				label="COM_OFFICE_OFFICE_ADDRESS_LABEL"
				description="COM_OFFICE_OFFICE_ADDRESS_DESC"
				size="40"
				class="inputbox"
				required="true"
				default=""
				/>
		<field
				name="note"
				type="text"
				label="COM_OFFICE_OFFICE_NOTE_LABEL"
				description="COM_OFFICE_OFFICE_NOTE_DESC"
				size="40"
				class="inputbox"
				default=""
				/>
		<field
				name="phones"
				type="text"
				label="COM_OFFICE_OFFICE_PHONES_LABEL"
				description="COM_OFFICE_OFFICE_PHONES_DESC"
				size="40"
				class="inputbox"
				required="true"
				default=""
				/>
		<field
				name="email"
				type="text"
				label="COM_OFFICE_OFFICE_EMAIL_LABEL"
				description="COM_OFFICE_OFFICE_EMAIL_DESC"
				size="40"
				class="inputbox"
				default=""
				/>
	</fieldset>
</form>
